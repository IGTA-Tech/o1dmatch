# Demo Mode Environment Variables
# Copy this file to .env.local for local demo development

# Enable demo mode - this makes the app use mock data instead of real APIs
NEXT_PUBLIC_DEMO_MODE=true

# Site URL (update for your deployment)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# These are placeholder values - not used in demo mode but required by the app
NEXT_PUBLIC_SUPABASE_URL=https://demo.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=demo-anon-key

